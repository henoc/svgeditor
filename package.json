{
  "name": "vscode-css-properties",
  "displayName": "Preview CSS Properties Sample",
  "description": "A sample illustrating the use of TextContentProviders and the `vscode.previewHtml` command, introduce in 0.10.7",
  "version": "0.0.10",
  "publisher": "eg2",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-extension-samples/issues",
    "email": "egamma@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples.git"
  },
  "homepage": "https://github.com/Microsoft/vscode-extension-samples/tree/master/textdocumentprovider-sample/README.md",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^0.10.7"
  },
  "activationEvents": [
    "onCommand:extension.openSvgEditor"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openSvgEditor",
        "title": "Open SVG Editor"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.openSvgEditor",
          "when": "resourceLangId == svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "tslint": "tslint -c tslint.json src/extension.ts",
    "pretest": "tsc test/*.ts -m amd --outFile out/test.js",
    "test": "ava out/test.js --verbose"
  },
  "devDependencies": {
    "@types/node": "*",
    "ava": "^0.23.0",
    "typescript": "^2.1.4",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "@types/ejs": "^2.3.33",
    "ejs": "^2.5.7"
  }
}
